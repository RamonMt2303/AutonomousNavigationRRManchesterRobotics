            <origin xyz="${camera_base_mesh_pos_x} ${camera_base_mesh_pos_y} ${camera_base_mesh_pos_z}" rpy="${camera_base_mesh_pos_r} ${camera_base_mesh_pos_p} ${camera_base_mesh_pos_w}"/>
