    <link name="jetson_link">
        <visual>
            <origin xyz="${jetson_mesh_pos_x} ${jetson_mesh_pos_y} ${jetson_mesh_pos_z}" rpy="${jetson_mesh_pos_r} ${jetson_mesh_pos_p} ${jetson_mesh_pos_w}"/>
