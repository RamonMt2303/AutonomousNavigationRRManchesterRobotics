            </geometry>
        </collision>

        <inertial>
            <origin xyz="${camera_base_inertia_pos_x} ${camera_base_inertia_pos_y} ${camera_base_inertia_pos_z}" rpy="${camera_base_inertia_pos_r} ${camera_base_inertia_pos_p} ${camera_base_inertia_pos_w}"/>
            <mass value="${camera_base_mass}"/>
