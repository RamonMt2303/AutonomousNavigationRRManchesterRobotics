            <origin xyz="${powerbank_mesh_pos_x} ${powerbank_mesh_pos_y} ${powerbank_mesh_pos_z}" rpy="${powerbank_mesh_pos_r} ${powerbank_mesh_pos_p} ${powerbank_mesh_pos_w}"/>
